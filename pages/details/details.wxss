page{
    background: #f6f6f6;
}
.goods_detailbox{
    margin-bottom: 110rpx;
}
.swiper-container {
    position: relative;
    margin: 0 auto;
}
.swiper-container .swiper {
    height:750rpx;
    width: 750rpx;
    margin: 0 auto;
}
.swiper-container .swiper .img {
    width: 100%;
    height: 100%;
}
.swiper-container .dots {
    position: absolute;
    left: 0;
    right:0 ;
    margin: 0 auto;
    bottom: -30rpx;
    display: flex;
    justify-content: center;
}
.swiper-container .dots .dot {
    margin: 30rpx 10rpx 0;
    width: 15rpx;
    height: 15rpx;
    background: #bcbcbc;
    border-radius: 50%;
    transition: all 0.6s;
    font: 300 18rpx/28rpx "microsoft yahei";
    text-align: center;
}
.swiper-container .dots .dot.active {
    background: #ff4949;
    color: #fff;
}
.detail_box{
    padding-top:50rpx; 
    padding-bottom:10rpx;
    background: #fff;
}
.det_border{
    margin-top:10rpx;
    padding-top:10rpx;
}
.detail_content{
    width: 710rpx;
    margin: 20rpx auto;
    background: #fff;
}
.detail_contentList{
    margin:20rpx auto;
    padding:0 20rpx;
    box-sizing:border-box;
    background:#fff;

}
.detail_content image{
    width: 100% !important;
}
.nameAndsell,.goods_detail,.momeyAndcar{
    display: flex;
    justify-content: space-between;
}
.nameAndsell{
    align-items: flex-end;
}
.name{
    font-size:36rpx;
    width: 480rpx;
    flex-shrink:0; 
    display:-webkit-box;
    overflow:hidden; 
    -webkit-line-clamp:1; 
    -webkit-box-orient:vertical;
}
.sell{
    font-size:24rpx;
    color:#9d9d9d;
    flex-shrink: 0;
    text-align:right;
}
.goods_detail{
    width: 100%;
    margin:15rpx 0;
    font-size:24rpx;
    color:#9d9d9d;
    align-items: flex-start;
}
.goods_detail_text{
    width: 480rpx;
    flex-shrink: 0;
}
.goods_detail_address{
    overflow: hidden;
    width:210rpx;
    text-align:right;
    flex-shrink:0;
    white-space:nowrap;
    text-overflow:ellipsis;
}
.goods_momey{
     width: auto;
     padding: 20rpx 0;
     display: flex;
     justify-content: space-between;
     align-items: center;
     font-size: 30rpx;
     color: #9d9d9d;
}
.member{
    color: #FF4F4F;
    padding-right:10rpx
}
.originally{
    color: #9d9d9d;
    padding-left:10rpx
}
.jionCar{
    width: 60rpx;
    height: 60rpx;
    border-radius:50%;
    text-align: center;
    line-height: 60rpx; 
    background:#3B8BFE;
    color:#fff; 
}
.jionCar .iconfont {
    font-size:35rpx;
}

.footer_box{
    position: fixed;
    height: 115rpx;
    width: 100%;
    padding:0 20rpx;
    box-sizing: border-box;
    bottom: 0;
    box-shadow: 0 0 20rpx #d9d9d9;
    background: #fff;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.customer_service{
    height:100rpx;
    padding:0;
    width:100rpx;
    margin:0;
    background:#fff;
}
.btn-hover{background-color: #fff;} 
.customer_service .iconfont{
    font-size:50rpx;
    color:#666;
    height:45rpx;
    line-height:60rpx;
}
.service_text{
    font-size:22rpx;
    color:#666;

}
.details_btn{
    display:flex;
    justify-content: space-between;
    width:470rpx;
}
button::after{
    border: none;
}
.btn{
    height: 85rpx;
    border-radius:5rpx;
    color:#fff;
    padding:0;
    width:220rpx;
    text-align: center;
    line-height: 85rpx;
    font-size: 24rpx;
}
.jion {
    background:#FE7B89;
}
.pay{
    background: #3B8BFE;
}
.detail_title{
    font-size:36rpx;
    font-weight:600;
    margin: 20rpx auto
}
.goods_introduce{
    font-size:24rpx;
    line-height:1.7em;
    text-indent:2em;
    color:#333;
}
.p{
    margin-bottom: 20rpx;
}
.p:last-child{
    margin: 0;
}
.img{
    width: 100%;
    text-indent:0;
    text-align: center;
    margin-bottom: 20rpx;
}
.img image{
    width: 100%;
    height: 100%;
}
.modal_box{
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    bottom:0;
    right: 0;
    margin: auto;
    background: rgba(0, 0, 0, .5) ;
    display: none;
}
.hidemodal{
    display: block;
}
.bottom_modal{
    position: absolute;
    width: 100%;
    height: 590rpx;
    background: #fff;
    border-radius:20rpx 20rpx 0 0 ; 
    bottom: -590rpx;
}
.modal_goods{
    border-bottom:1rpx solid #d9d9d9;
    padding-bottom:20rpx; 
}
.modal_goods:last-child{
    border:none;
    padding-bottom:none;
}
.modal_top,.modal_selsectnum,.modal_btn{
    display: flex;
    justify-content: space-between;
    width: 710rpx;
    margin: 0 auto;
    padding:0 15rpx;
    box-sizing:border-box;
}
.modal_top{
    margin-top:30rpx ;
    margin-bottom:20rpx;
}
.modal_img{
    width: 250rpx;
    height: 250rpx;
    border:1rpx solid #d9d9d9;
    overflow: hidden;
    border-radius:10rpx;
}
.modal_img image{
    width: 100%;
    height: 100%;
}
.modal_cen{
    width: 360rpx;
    height: 250rpx;
    position: relative;
}
.modal_name{
    font-size: 32rpx;
    color: #000;
    display:-webkit-box;
    overflow:hidden;
    -webkit-line-clamp:2;
    -webkit-box-orient:vertical;
}
.yuanjia{
    font-size: 26rpx;
    color: #9d9d9d;
    margin-top:20rpx; 
}
.modal_selsectnum{
    margin: 50rpx auto;
}
.modal_title{
    font-size:28rpx;
}
.modal_num{
    width: 150rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.modal_num .iconfont{
    font-size:40rpx;
    color: #000;
    width: 40rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    display: inline-block;
    border-radius:50%; 
}
.icon-jia{
    background: #cdcdcd;
    border: 1rpx solid #cdcdcd;
}
.icon-jianhao{
    background: #fff;
    border: 1rpx solid #999;
}
.modal_btn{
    margin-top:20rpx; 
}
.in_pay_btn{
    color:#fff;
    width:335rpx;
    height:80rpx;
    line-height:80rpx;
    font-size:28rpx;
    padding:0;
    margin:0;
    letter-spacing:10rpx;
    text-align:center;
    border-radius:30rpx;
}
.in_pay_blue{
    background: #3B8BFE;
}
.in_pay_red{
    background: #FE7B89;
}
.all_money_box{
    position:absolute;
    width:100%;
    font-size:30rpx;
    color:#9d9d9d;
    text-align:right;
    bottom:0;
}
.all_money_box text{
    color: #ff5757;
    font-size:40rpx;
}
.center_btn{
    justify-content:center;
}
/* 选项卡 */
.menu_box{
    display: flex;
    height: 70rpx;
    padding-top:15rpx;
    border-bottom:1rpx solid #cdcdcd;
    background:#fff;
}
.menu{
    flex: 1;
    line-height: 50rpx;
    text-align: center;
    border: #cdcdcd;
    color: #9d9d9d;
    font-size: 36rpx;
    position: relative;
}
.menu:first-child::after{
    position: absolute;
    content: "";
    height: 1em;
    width: 1rpx;
    background: #d9d9d9;
    top: 0;
    bottom: 0;
    margin: auto;
    right: 0;
}
.tab{
    background: #f2f2f2;
}
.borders{
    padding-bottom:20rpx; 
    color: #3c8cff;
    border-bottom:5rpx solid #3e8cff ;
}
.detail_header{
    display: flex;
    justify-content: space-between;
    align-items:center;
    padding: 20rpx 0;

}
.Other_user{
    display: flex;
    align-items: center;
    flex-shrink: 0;
}
.otherPic{
    width: 80rpx;
    height: 80rpx;
    border-radius:50%;
    margin-right: 20rpx; 
    flex-shrink: 0;
    overflow: hidden;
}
.otherNane{
    width: 400rpx;
    font-size: 32rpx;
    color: #333;
    flex-shrink: 0;
}
.otherPic image{
    width: 100%;
    height: 100%;
}
.score_box{
    width: 200rpx;
    display: flex;
    color: #ffc03a;
}
.score_box .iconfont {
    margin-left:5rpx; 
}
.detail_evaluate{
    text-indent:2em;
    font-size:28rpx;
    line-height:2em;
    color:#848484;
    margin:10rpx auto;
}
.detail_picture{
    display: flex;

    flex-wrap: wrap;
}
.picture_list{
    width: 230rpx;
    height: 230rpx;
    flex-shrink: 0;
    margin-bottom: 10rpx;
    margin-right:10rpx; 
}
.picture_list:nth-child(3),.picture_list:nth-child(6),.picture_list:nth-child(9){
    margin-right:0; 
}
.picture_list image{
    width: 100%;
    height: 100%;
}
.detail_time{
    text-align: right;
    font-size: 28rpx;
    color: #848484;
    padding: 20rpx 0;
}
.goods_address{
    width: 100%;
    margin:15rpx 0;
    font-size:24rpx;
    color:#9d9d9d;
    display:-webkit-box; 
    overflow:hidden;
    -webkit-line-clamp:3;
    -webkit-box-orient:vertical;
}