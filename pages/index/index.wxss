.t-notice{
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    margin:  auto;
    background: rgba(0, 0, 0, .65);
    z-index: 9999
}
.t-image{
    width: 350rpx;
    height: 700rpx;
    margin: 0 auto;
}
.t-notice image{
    width: 100%;
    height: 100%;
}
.t-button{
    width: 220rpx;
    height: 70rpx;
    margin: 20rpx auto;
    position: relative;
}
.t-button button{
    height: 70rpx;
    line-height: 70rpx;
    background: #3c8bff;
    color: #fff;
    border-radius:50rpx;
    font-size:26rpx;
}
.t-button::before{
    position: absolute;
    width: 220rpx;
    height:70rpx;
    content: "";
    top: 10rpx;
    background: transparent;
    border: 3rpx solid #fff;
    border-radius:50rpx;
    box-sizing: border-box;
}
.recommend{
    margin: 30rpx auto;
}

.swiper-container{
  position: relative;
  width: 700rpx;
  margin: 0 auto;
  box-shadow: 0 0 30rpx #ccc;
}
.swiper-container .swiper{
  height: 350rpx;
}
.swiper-container .swiper .img{
  width: 100%;
  height: 100%;
}
.swiper-container .dots{
  position: absolute;
  right: 0;
  left: 0;

  display: flex;
  justify-content: center;
}
.swiper-container .dots .dot{
  margin:30rpx 10rpx 0;
  width:15rpx;
  height: 15rpx;
  background: #bcbcbc;
  border-radius: 50%;
  transition: all .6s;
  font: 300 18rpx/28rpx "microsoft yahei";
  text-align: center;
}
.swiper-container .dots .dot.active{
  background: #ff4949;
  color:#fff;
}
.section{
    margin:60rpx auto 0;
    padding: 20rpx 0;
}
.section-title{
    text-align: center;
    position: relative;
    margin: 0 auto;
}
.section-title text{
