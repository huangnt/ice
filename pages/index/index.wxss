.body-view{
    padding: 20rpx;
    text-align: center;
    font-size: 30rpx;
    color: #999;
    position: relative;
}
.body-view::after{
    position: absolute;
    content: "";
    width: 5em;
    height: 1rpx;
    background: #999;
    right: 3em;
    top: 0;
    bottom: 0;
    margin: auto;
}
.body-view::before{
    position: absolute;
    content: "";
    width: 5em;
    height: 1rpx;
    background: #999;
    left: 3em;
    top: 0;
    bottom: 0;
    margin: auto;
}