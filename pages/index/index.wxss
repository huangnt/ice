.goodslist{
    width: 685rpx;
    margin:30rpx auto;
    border-radius:10rpx; 
    box-shadow: 0 0 20rpx #d9d9d9;
    background:#fff;

}
.goodsPic{
    width: 685rpx;
    height: 365rpx;
    position: relative;
}
.goodsPic image{
    width: 100%;
    height: 100%;
    border-radius:10rpx 10rpx 0 0; 
}
.tagone{
    position: absolute;
    left: -10rpx;
    top: 20rpx;
    width: 90rpx;
    height: 45rpx;
    background: #ffdfa2;
    line-height: 45rpx;
    font-size: 30rpx;
    color: #333;
}
.tagone text{
    position: relative;
    width: 100% ;
    height: 100%;
    display: inline-block;
    text-indent: .5em;
}
.tagone text::after{
    position: absolute;
    content: "";
    width: 0;
    height: 0;
    border-top: 45rpx solid #ffdfa2;
    border-right: 45rpx solid transparent;
    right: -35rpx;
    top:0rpx;
    z-index: 0;
}
.tagone text::before{
    position: absolute;
    content: "";
    width: 0;
    height: 0;
    border-bottom: 45rpx solid #ffdfa2;
    border-right: 45rpx solid transparent;
    right: -35rpx;
    bottom:0rpx;
    z-index: 0;
}

.tagtwo{
    position: absolute;
    left: -10rpx;
    top: 80rpx;
    width: 200rpx;
    height:40rpx;
    background:#3B8BFE;
    font-size: 24rpx;
    color: #fff;
}
.tagtwo text{
    position: relative;
    width: 100% ;
    height: 100%;
    display: inline-block;
    line-height: 40rpx;
    text-indent: .5em;
}
.tagtwo text::after{
    position: absolute;
    content: "";
    width: 0;
    height: 0;
    border-top: 40rpx solid #3B8BFE;
    border-right: 40rpx solid transparent;
    right: -35rpx;
    top:0;
    z-index: 0;
}
.tagtwo text::before{
    position: absolute;
    content: "";
    width: 0;
    height: 0;
    border-bottom: 40rpx solid #3B8BFE;
    border-right: 40rpx solid transparent;
    right: -35rpx;
    bottom:0;
    z-index: 0;
}

