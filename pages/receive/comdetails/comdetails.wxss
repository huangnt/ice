.bannerbg{
    width: 100%;
    height: 370rpx;
    position: absolute;
    top: 0;
    left: 0;
    z-index: -1;
}
.bannerbg image{
    height: 100%;
    width: 100%;
}
.takedetails{
    background: #fff;
    border-radius:50rpx 50rpx 0 0; 
    margin-top:300rpx; 
    overflow: hidden;
}
.takedetails_box{
    padding:0 44rpx;
    border-bottom:1rpx solid #eee;
    box-sizing: border-box;
}
.other_content{
    padding: 35rpx 0;
}
.take_Title{
    font-size: 36rpx;
    margin-top:60rpx; 
}
.take_state{
    display: flex;
    align-items: center;
    padding:30rpx 0;
}
.iconfont{
    font-size: 30rpx; 
    color:#3B8AFF;
}
.state_Text{
    font-size:28rpx;
    color:#FF4A48;
    margin-left:10rpx 
}

.goodsList{
    padding-bottom: 35rpx;
}
.goodsbox{
    height: 215rpx;
    display:flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom:55rpx;
}
.goods_img{
    height: 215rpx;
    width: 215rpx;
}
.goods_img image{
    width: 100%;
    height: 100%;
}
.goods_content{
    width:390rpx;
}
.goods_name{
    font-size: 32rpx;
    margin-bottom: 15rpx;
    display:-webkit-box; 
    overflow:hidden; 
    -webkit-line-clamp:1;
    -webkit-box-orient:vertical;
}
.goods_money,.goods_num{
    font-size: 26rpx;
    color: #9d9d9d;
    margin-top: 10rpx;
}
.genduo{
    width:215rpx;
    height:45rpx;
    font-size:28rpx;
    line-height:45rpx;
    text-align:center;
    margin:10rpx auto;
    color:#9d9d9d;
    border:1rpx solid #d9d9d9;
}
.goods_hei{
    max-height:500rpx; 
    overflow:hidden;
}
.goods_heiOne{
    max-height:none; 
}
.take_smallTitle{
    display: flex;
    justify-content: space-between;
    align-items: flex-end;
    padding:.5em 1em;

}
.smallTitle{
    font-size: 35rpx;
    color: #333;
    
}
.take_Num{
    font-size:30rpx;
    color: #9d9d9d; 
}
.take_goodsLise{
    padding: 0 1em;
    border-bottom:1rpx solid #eee; 
}
.take_goodsLise:last-child{
    border-bottom:none;
}
.confirm_t{
    color:#000;
    font-size:24rpx;
    margin-bottom:35rpx;
}
.confirm_t:last-child{
    margin-bottom:0;
}
.c_text{
    color: #9d9d9d;
    font-weight: 400;
}
.c_btn{
    color: #9d9d9d;
    font-weight: 400;
    border:1rpx solid #9d9d9d;
    display: inline-block;
    border-radius: 30rpx;
    padding:5rpx 20rpx;
    margin-left:20rpx;
}
.c_momey{
    color: #F14A61;
}
.confirm_btn{
    margin-top: 50rpx;
    display:flex;
}
.confirm_btn button{
    width: 300rpx;
    height:70rpx;
    line-height:70rpx;
    font-size:30rpx;
    border-radius:5rpx; 
    letter-spacing: 5rpx;
}
.red_btn{
    background: #ff7c8b;
    color:#fff;
}
.fff_btn{
    background: #fff;
    color: #333;
    border: 1rpx solid #8f8e8c;
}
.people_num{
    display: flex;
    align-items: flex-start;
}

.people_box{
    display: flex;
    align-items: center;
    max-height: 60rpx;
    flex-wrap: wrap;
    overflow: hidden;
}
.userList{
    display:flex;
    align-items: center;
}

.user{
    width: 35rpx;
    height: 35rpx;
    border-radius:50%; 
    overflow: hidden;
    margin-right:10rpx; 
    margin-bottom:10rpx; 
    flex-shrink: 0;
}
.user image{
    width: 100%;
    height: 100%;
}
.inall{
     font-size: 28rpx;
    font-weight: 400;
    color: #9d9d9d;
}
.on{
    max-height: none;
}